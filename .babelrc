{
    "presets": [
        ["@babel/preset-env",
            {
                "targets": {
                    "browsers": ["last 30 versions", "> 2%", "Firefox >= 10"]
                }
            }
        ]
    ],
    "plugins": [
        ["@babel/plugin-proposal-decorators", { "legacy": true}],
        ["@babel/plugin-transform-runtime", {
            "corejs": 2
        }],
        [
            "@babel/plugin-transform-modules-commonjs"
        ]
    ]
}